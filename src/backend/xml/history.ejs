<!-- XML DEFINITION -->
<!DOCTYPE document [
<!ELEMENT document (title,session*)>
<!ELEMENT title (#PCDATA)>
<!ELEMENT session (id,trainer,date,time,name,maxspace,space)*>
<!ELEMENT id (#PCDATA)>
<!ELEMENT trainer (#PCDATA)>
<!ELEMENT date (#PCDATA)>
<!ELEMENT time (#PCDATA)>
<!ELEMENT name (#PCDATA)>
<!ELEMENT maxspace (#PCDATA)>
<!ELEMENT space (#PCDATA)>
]>












<document>
    <title>Complete History of classes</title>
<% for (var i = 0; i < sessions.length; i++) { %>
    <session>
        <id><%- sessions[i].session_id %></id>
        <trainer><%- sessions[i].name %></trainer>
        <date><%- sessions[i].date %></date>
        <time><%- sessions[i].time %></time>
        <name><%- sessions[i].session_name %></name>
        <maxspace><%- sessions[i].max_space %></maxspace>
        <space><%- sessions[i].booked %>/<%- sessions[i].max_space %></space>
    </session>

<% } %>

</document>
